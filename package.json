{
  "name": "jbod",
  "version": "0.4.8",
  "description": "JavaScript Binary Serialization and deserialization. Support for more JS data types",
  "type": "module",
  "types": "./dist/mod.d.ts",
  "scripts": {
    "ci:test": "vitest run",
    "ci:build": "pnpm rollup -c",
    "build": "pnpm rollup -c && pnpm run api:check -l",
    "test": "vitest",
    "type:check": "tsc",
    "api:check": "api-extractor run -c api-extractor.jsonc -v"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.39.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.15",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asnowc/jbod"
  },
  "exports": {
    ".": "./dist/mod.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "provenance": true
  },
  "keywords": [
    "json",
    "serialize",
    "protobuf",
    "bson"
  ],
  "author": {
    "name": "Eavid",
    "url": "https://github.com/eavidy"
  },
  "license": "MIT",
  "dependencies": {}
}
